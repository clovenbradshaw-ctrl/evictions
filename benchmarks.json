{
  "metadata": {
    "dataset_name": "Nashville Eviction Benchmark Dataset",
    "version": "1.1",
    "created_date": "2024-12-24",
    "last_updated": "2024-12-24",
    "purpose": "Enable comparison of Nashville eviction data against similar US cities and national averages",
    "primary_sources": [
      "Princeton Eviction Lab (evictionlab.org)",
      "Eviction Tracking System (ETS) 2024 Report",
      "U.S. Census Bureau American Community Survey 2019-2023",
      "Census Bureau American Housing Survey",
      "State and local court records",
      "Collinson et al. 'Eviction and Poverty in American Cities' (QJE 2024)"
    ],
    "metric_definitions": {
      "filing_rate": {
        "formula": "(eviction_filings / renter_households) × 100",
        "description": "Percentage of renter households facing an eviction filing in a given period",
        "interpretation": "Measures threat of eviction; includes serial filings to same household"
      },
      "judgment_rate_pct_of_filings": {
        "formula": "(eviction_judgments / eviction_filings) × 100",
        "description": "Percentage of filed cases that result in an eviction judgment/order",
        "interpretation": "Measures court outcome rates; varies by jurisdiction and tenant representation",
        "national_benchmark": "Approximately 50% nationally per Collinson et al."
      },
      "eviction_rate_pct_of_renters": {
        "formula": "(eviction_judgments / renter_households) × 100",
        "description": "Percentage of renter households receiving an eviction judgment in a given period",
        "interpretation": "Measures actual displacement rate; the 'true' eviction rate",
        "national_benchmark": "2.3-3.13% nationally (2000-2016 range)"
      },
      "relationship": "eviction_rate = filing_rate × judgment_rate_pct_of_filings"
    }
  },

